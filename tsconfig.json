{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "target": "es6",
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  },
  "include": ["src/*.ts"],
  "exclude": []
}
